{
    "name": "peehaa/awesome-feed",
    "description": "Converts any resource into an RSS feed",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "amphp/artax": "^3.0",
        "amphp/http-server-router": "^1.0",
        "amphp/log": "^1.0",
        "amphp/redis": "^0.3.3",
        "amphp/websocket-server": "dev-master",
        "cocur/slugify": "^3.1",
        "codecollab/form": "^1.0",
        "codecollab/http": "^1.3",
        "codecollab/i18n": "^1.0.1",
        "composer/composer": "^1.7",
        "filp/whoops": "^2.2",
        "league/climate": "^3.4",
        "nikic/fast-route": "^1.3",
        "rdlowrey/auryn": "^1.4",
        "robmorgan/phinx": "^0.10.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "PeeHaa\\AwesomeFeed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeeHaa\\AwesomeFeedTest\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "PeeHaa\\AwesomeFeed\\Install\\PostInstall::run"
        ],
        "post-update-cmd": [
            "PeeHaa\\AwesomeFeed\\Install\\PostInstall::run"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
